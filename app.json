{
  "expo": {
    "name": "my-app-andrew",
    "slug": "my-app-andrew",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.andrewandersonorg.myappandrew"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.andrewandersonorg.myappandrew"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "0c173ea1-0ac4-495f-928a-3455f795bfcd"
      }
    },
    "owner": "andrewanderson-org",
    "updates": {
      "url": "https://u.expo.dev/0c173ea1-0ac4-495f-928a-3455f795bfcd"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
