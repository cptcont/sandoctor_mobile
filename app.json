{
  "expo": {
    "name": "sandoctor",
    "slug": "sandoctor",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon-1.5.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.andrewandersonorg.sandoctor",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "googleServicesFile": "./google-services.json",

      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-1.5.png",
        "backgroundColor": "#ffffff"
      },

      "notification": {
        "icon": "./assets/images/notification-icon-1.5.png",
        "color": "#081A51"
      },

      "package": "com.andrewandersonorg.sandoctor",
      "permissions": [
        "CAMERA",
        "CALL_PHONE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-1.5.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon-1.5.png",
          "color": "#081A51"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "andrewanderson-org",
    "updates": {
      "url": "https://u.expo.dev/0ec8419e-5e62-4697-81a1-a4214a4485dc"
    },
    "runtimeVersion": "1.0.5",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0ec8419e-5e62-4697-81a1-a4214a4485dc"
      }
    }
  }
}
